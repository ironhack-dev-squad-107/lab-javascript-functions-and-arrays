{
    "extends" : "airbnb-base/legacy",
    "rules": {
        "func-names": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "vars-on-top": "off",
        "no-multi-spaces": "off",
        "no-octal": "off"
    }
}

