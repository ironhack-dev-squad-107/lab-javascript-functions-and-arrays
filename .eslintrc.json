{
    "extends" : "airbnb-base/legacy",
    "plugins": [ "jasmine" ],
    "env": {
        "jasmine": true
    },
    "rules": {
        "func-names": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "vars-on-top": "off"
    }
}